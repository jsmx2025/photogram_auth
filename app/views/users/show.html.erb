<div class="page-header">
  <h3><%= @user.username %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User ID</dt>
      <dd><%= @user.id %></dd>
      <dt>Email</dt>
      <dd><%= @user.email %></dd>
      <dt>Photos</dt>
      <dd><%= @photo.caption %></dd>
      <dt>Photo ID</dt>
      <dd><%= @photo.id %></dd>
      <dd><img src="<%= @photo.image %>"></dd>
    </dl>
  </div>

</div>
